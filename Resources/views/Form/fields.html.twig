
{% block entity_double_list_widget %}
    {{ snide_extra_form_addDoubleList({
            'id': id,
            'search': search,
            'minSize': minSize,
            'selectedPosition': selectedPosition,
            'moveOptions': moveOptions,
            'labelTop': labelTop,
            'labelBottom': labelBottom,
            'labelUp': labelUp,
            'labelDown': labelDown,
            'labelSort': labelSort,
            'maxSelected': maxSelected,
            'labelsx': labelsx,
            'labeldx': labeldx,
            'autoSort': autoSort,
            'caseSensitive': caseSensitive,
            'delay': delay
        }) 
    }}
    {% spaceless %}
        {% if expanded %}
            <ul {{ block('widget_container_attributes') }}>
            {% for child in form %}
                <li>
                    {{ form_widget(child) }}
                    {{ form_label(child) }}
                </li>
            {% endfor %}
            </ul>
        {% else %}
            {# laisse le widget choice afficher la balise select #}
            {{ block('choice_widget') }}
        {% endif %}
    {% endspaceless %}
{% endblock %}